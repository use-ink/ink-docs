{
  "retryCount": 12,
  "fallbackRetryDelay": "60s",
  "timeout": "30s",
  "aliveStatusCodes": [429, 200, 403, 0],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "https://use.ink/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://crates.io"],
      "headers": {
        "Accept": "text/html"
      }
    }
  ],
  "ignorePatterns": [
    {
      "pattern": "^https?://twitter.com"
    },
    {
      "pattern": "^https?://x.com"
    },
    {
      "pattern": "\\{useBaseUrl\\("
    },
    {
      "pattern": "^https?://substrate\\.io"
    },
    {
      "pattern": "^https?://polkadot\\.network"
    }
  ]
}
