{
  "name": "ink-docs",
  "version": "0.0.0",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "docusaurus": "docusaurus",
    "clear": "docusaurus clear",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "start": "docusaurus start --port 8080",
    "lint": "yarn eslint .",
    "fmt": "yarn prettier --write \"docs/**/*.{js,jsx,ts,tsx,mdx}\" \"src/**/*.{js,jsx,ts,tsx,mdx,css}\" && yarn eslint --fix .",
    "build": "yarn lint && docusaurus build",
    "build:release": "yarn lint && ci-ghact-docs",
    "docs": "yarn build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve --port 8080",
    "cspell": "cspell \"**/*.{md,mdx}\"",
    "migrate-image-links": "ts-node src/scripts/migrate-image-links.ts",
    "create-llm-file": "yarn repomix",
    "postinstall": "yarn create-llm-file"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.7.0",
    "@docusaurus/tsconfig": "^3.7.0",
    "@docusaurus/types": "^3.7.0",
    "@types/react-google-recaptcha": "^2.1.9",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "cspell": "^8.3.2",
    "eslint": "^8.56.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.37.4",
    "prettier": "^3.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "^3.7.0",
    "@docusaurus/plugin-client-redirects": "^3.7.0",
    "@docusaurus/preset-classic": "^3.7.0",
    "@easyops-cn/docusaurus-search-local": "^0.46.1",
    "@headlessui/react": "^2.2.0",
    "@lottiefiles/lottie-player": "^2.0.3",
    "@mdx-js/react": "^3.0.0",
    "@phosphor-icons/react": "^2.1.7",
    "@radix-ui/react-slot": "^1.1.2",
    "@rive-app/react-canvas": "^4.18.3",
    "autoprefixer": "^10.4.17",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "hamburger-react": "^2.5.2",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.479.0",
    "motion": "^12.5.0",
    "ogl": "^1.0.11",
    "postcss": "^8.4.33",
    "prism-react-renderer": "^2.3.1",
    "react-google-recaptcha": "^3.1.0",
    "react-icons": "^5.5.0",
    "react-spring": "^9.7.5",
    "repomix": "^0.3.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.167.1",
    "usehooks-ts": "^2.12.1"
  }
}
